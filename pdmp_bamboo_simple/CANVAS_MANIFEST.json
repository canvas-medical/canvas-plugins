{
    "sdk_version": "0.1.4",
    "plugin_version": "0.0.1",
    "name": "pdmp_bamboo_simple",
    "description": "Simple PDMP Bamboo Plugin - Streamlined version for testing PDMP requests with BambooHealth PMP Gateway",
    "components": {
        "protocols": [
            {
                "class": "pdmp_bamboo_simple.protocols.my_protocol:PDMPTestRequestProtocol",
                "description": "PDMP Test Request button that extracts Canvas data and sends PDMP requests to BambooHealth test environment",
                "data_access": {
                    "event": "SHOW_NOTE_HEADER_BUTTON",
                    "read": [
                        "v1.Note",
                        "v1.Patient",
                        "v1.Staff",
                        "v1.Organization",
                        "v1.PracticeLocation"
                    ],
                    "write": [
                        "StructuredAssessmentCommand"
                    ]
                }
            },
            {
                "class": "pdmp_bamboo_simple.utils.report_api:ReportAPI",
                "description": "API endpoint for generating PDMP reports"
            }
        ],
        "commands": [],
        "content": [],
        "effects": [],
        "views": []
    },
    "secrets": [
        "TEST_PDMP_API_URL",
        "TEST_PDMP_API_USERNAME",
        "TEST_PDMP_API_PASSWORD"
    ],
    "tags": {
        "category": "clinical",
        "integration": "pdmp",
        "environment": "test"
    },
    "references": [
        "BambooHealth PMP Gateway API Documentation"
    ],
    "license": "MIT",
    "diagram": false,
    "readme": "./README.md"
}