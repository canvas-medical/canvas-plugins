{
  "sdk_version":"0.1.4",
  "plugin_version":"0.0.2",
  "name":"missing_coverage_label",
  "description": "Adds a 'Missing Coverage' label to appointments for patients without coverage, and removes the label when coverage is added.",
  "components":{
    "protocols":[
      {
        "class":"missing_coverage_label.protocols.missing_coverage_label:AppointmentLabelsProtocol",
        "description": "Adds a 'Missing Coverage' label to new appointments for patients without coverage and removes the label from all of a patient's appointments when coverage is subsequently added.",
        "data_access":{
          "event":"read",
          "read":[
            "Patient",
            "Coverage",
            "Appointment"
          ],
          "write":[
            "ADD_APPOINTMENT_LABEL",
            "REMOVE_APPOINTMENT_LABEL"
          ]
        },
        "meta":{
          "RESPONDS_TO":[
            "APPOINTMENT_CREATED",
            "COVERAGE_CREATED"
          ]
        }
      }
    ],
    "commands":[],
    "content":[],
    "effects":[],
    "views":[]
  },
  "secrets":[],
  "tags":{
