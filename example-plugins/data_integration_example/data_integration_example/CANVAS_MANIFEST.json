{
    "sdk_version": "0.1.4",
    "plugin_version": "0.1.0",
    "name": "data_integration_example",
    "description": "Example plugin demonstrating Data Integration effects and document lifecycle events",
    "components": {
        "protocols": [
            {
                "class": "data_integration_example.protocols.data_integration_handler:DataIntegrationHandler",
                "description": "Protocol that demonstrates Data Integration effects on DOCUMENT_RECEIVED and logs all document lifecycle events",
                "data_access": {
                    "event": "read",
                    "read": [
                        "integration_tasks",
                        "patients",
                        "staff",
                        "teams"
                    ],
                    "write": [
                        "integration_tasks"
                    ]
                },
                "meta": {
                    "RESPONDS_TO": [
                        "DOCUMENT_RECEIVED",
                        "DOCUMENT_LINKED_TO_PATIENT",
                        "DOCUMENT_CATEGORIZED",
                        "DOCUMENT_REVIEWER_ASSIGNED",
                        "DOCUMENT_FIELDS_UPDATED",
                        "DOCUMENT_REVIEWED",
                        "DOCUMENT_DELETED"
                    ]
                }
            }
        ],
        "commands": [],
        "content": [],
        "effects": [],
        "views": []
    },
    "tags": [],
    "secrets": [],
    "license": "NONE",
    "readme": "./README.md"
}
