{
    "sdk_version": "0.1.4",
    "plugin_version": "0.0.1",
    "name": "patient_portal_plugin",
    "description": "Patient Portal Plugin for Canvas",
    "url_permissions": [
        {
            "url": "http://localhost:8000/app/messaging",
            "permissions": ["ALLOW_SAME_ORIGIN", "SCRIPTS", "MICROPHONE", "CAMERA"]
        }
    ],
    "components": {
        "protocols": [
            {
                "class": "patient_portal_plugin.handlers.patient_portal_handler:PatientPortalHandler",
                "description": "The handler that listens for the patient portal `PATIENT_PORTAL__WIDGET_CONFIGURATION` and responds with the patient portal widgets",
                "data_access": {
                    "event": "",
                    "read": [],
                    "write": []
                }
            },
            {
                "class": "patient_portal_plugin.handlers.prescriptions_handler:PrescriptionsHandler",
                "description": "Handles the prescriptions plugin functionality, which enables patients to request prescription refills through the patient portal.",
                "data_access": {
                    "event": "",
                    "read": [],
                    "write": []
                }
            }
        ],
        "commands": [],
        "content": [],
        "effects": [],
        "views": []
    },
    "secrets": [
        "BACKGROUND_COLOR",
        "EMERGENCY_CONTACT",
        "ASSIGNED_TEAM"
    ],
    "tags": {},
    "references": [],
    "license": "",
    "diagram": false,
    "readme": "./README.md"
}
