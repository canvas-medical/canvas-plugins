{
    "sdk_version": "0.1.4",
    "plugin_version": "0.1.0",
    "name": "categorize_document_plugin",
    "description": "Example plugin that demonstrates how to use the CATEGORIZE_DOCUMENT effect to categorize documents in the Data Integration queue",
    "components": {
        "protocols": [
            {
                "class": "categorize_document_plugin.protocols.categorize_document_handler:CategorizeDocumentHandler",
                "description": "Handler that categorizes documents when they are received",
                "data_access": {
                    "event": "DOCUMENT_RECEIVED",
                    "read": [
                        "integration_tasks"
                    ],
                    "write": [
                        "integration_tasks"
                    ]
                }
            }
        ],
        "commands": [],
        "content": [],
        "effects": [],
        "views": []
    },
    "tags": [],
    "secrets": [],
    "license": "NONE",
    "readme": "./README.md"
