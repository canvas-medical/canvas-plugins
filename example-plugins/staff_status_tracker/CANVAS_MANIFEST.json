{
    "sdk_version": "0.1.4",
    "plugin_version": "0.1.0",
    "name": "staff_status_tracker",
    "description": "A plugin to track staffing status changes for a clinic",
    "components": {
        "protocols": [
            {
                "class": "staff_status_tracker.handlers.cron_task:StaffStatusCronTask",
                "description": "Weekly cron job to collect all staff status data",
                "data_access": {
                    "event": "CRON",
                    "read": [
                        "staff"
                    ],
                    "write": []
                }
            },
            {
                "class": "staff_status_tracker.handlers.event_handlers:StaffActivatedHandler",
                "description": "Handler for staff activation events",
                "data_access": {
                    "event": "STAFF_ACTIVATED",
                    "read": [
                        "staff"
                    ],
                    "write": []
                }
            },
            {
                "class": "staff_status_tracker.handlers.event_handlers:StaffDeactivatedHandler",
                "description": "Handler for staff deactivation events",
                "data_access": {
                    "event": "STAFF_DEACTIVATED",
                    "read": [
                        "staff"
                    ],
                    "write": []
                }
            },
            {
                "class": "staff_status_tracker.handlers.api:StaffStatusTriggerAPI",
                "description": "API endpoint to manually trigger staff status collection for testing",
                "data_access": {
                    "event": "SIMPLE_API_REQUEST",
                    "read": [
                        "staff"
                    ],
                    "write": []
                }
            }
        ],
        "commands": [],
        "content": [],
        "effects": [],
        "views": []
    },
    "tags": ["staff", "tracking", "cron"],
    "secrets": ["my_api_key"],
    "license": "NONE",
    "readme": "NONE"
}